<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <corners android:radius="@dimen/sx_dp23" />
            <solid android:color="@android:color/transparent" />
            <stroke android:width="@dimen/sx_dp_0_5" android:color="@color/main_color" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <corners android:radius="@dimen/sx_dp23" />
            <solid android:color="@color/color_f2ede7" />
            <stroke android:width="@dimen/sx_dp_0_5" android:color="@color/main_color_press" />
        </shape>
    </item>

</selector>